<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="multi.android.intent">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BasicAppRun">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UseOtherActivityTest"></activity>
        <activity android:name=".ExamIntent.ExamSecondActivity" />
        <activity android:name=".FirstIntent.ReturnDataFirstActivity" />
        <activity android:name=".FirstIntent.ReturnDataSecondActivity" />
        <activity android:name=".ExamIntent.ExamFirstActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
</manifest>